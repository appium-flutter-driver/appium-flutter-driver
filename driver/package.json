{
  "name": "appium-flutter-driver",
  "description": "Appium Flutter driver",
  "keywords": [
    "appium",
    "flutter"
  ],
  "version": "1.20.2",
  "author": "TruongSinh Tran-Nguyen <i@truongsinh.pro>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appium-userland/appium-flutter-driver.git"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=8"
  },
  "main": "./build/lib/driver.js",
  "types": "./build/lib/index.d.ts",
  "appium": {
    "driverName": "flutter",
    "automationName": "Flutter",
    "platformNames": [
      "iOS",
      "Android"
    ],
    "mainClass": "FlutterDriver"
  },
  "bin": {},
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "build",
    "!build/tsconfig.tsbuildinfo"
  ],
  "scripts": {
    "prepublishOnly": "cp ../README.md ../LICENSE ./",
    "prepare": "npm run clean && npm run build",
    "test": "echo no test",
    "build": "tsc -b",
    "clean": "rm -rf build",
    "lint": "tslint 'lib/**/*.ts'",
    "clean-dependency": "rm -rf node_modules && rm -f package-lock.json"
  },
  "peerDependencies": {
    "appium": "^2.0.0"
  },
  "devDependencies": {
    "@appium/schema": "^0.3.0",
    "@appium/tsconfig": "0.3.0",
    "@appium/types": "^0.13.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "json-schema": "^0.4.0",
    "tslint": "^6.1.3",
    "type-fest": "^4.0.0",
    "typescript": "^5.0.2",
    "semantic-release": "^20.1.3"
  },
  "dependencies": {
    "appium-adb": "^9.14.4",
    "appium-android-driver": "^5.2.2",
    "appium-ios-device": "^2.4.1",
    "appium-uiautomator2-driver": "^2.29.4",
    "appium-xcuitest-driver": "4.30.1",
    "asyncbox": "^2.3.1",
    "bluebird": "^3.1.1",
    "lodash": "^4.0.0",
    "portscanner": "2.2.0",
    "rpc-websockets": "^7.4.16"
  }
}
